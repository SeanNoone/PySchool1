   },
   "outputs": [],
   "source": [
    "figueroa_student_counts = figueroa_student_df[\"Student ID\"].count()\n",
    "figueroa_average_math_score = figueroa_student_df[\"math_score\"].mean()\n",
    "figueroa_average_reading_score = figueroa_student_df[\"reading_score\"].mean()\n",
    "figueroa_average = (figueroa_average_math_score + figueroa_average_reading_score)/2\n",
    "passing_math = figueroa_student_df[(figueroa_student_df['math_score']>70)].count()[\"name\"]\n",
    "figueroa_percent_passing_math = passing_math/figueroa_student_counts\n",
    "passing_reading = figueroa_student_df[(figueroa_student_df['reading_score']>70)].count()[\"name\"]\n",
    "figueroa_percent_passing_reading = passing_reading/figueroa_student_counts\n",
    "figueroa_type = schools_df.loc[schools_df[\"name\"] == \"Figueroa High School\", \"type\"]\n",
    "figueroa_budget = schools_df.loc[schools_df[\"name\"] == \"Figueroa High School\", \"budget\"]"
   ]
