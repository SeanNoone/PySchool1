   "source": [
    "griffin_student_counts = griffin_student_df[\"Student ID\"].count()\n",
    "griffin_average_math_score = griffin_student_df[\"math_score\"].mean()\n",
    "griffin_average_reading_score = griffin_student_df[\"reading_score\"].mean()\n",
    "griffin_average = (griffin_average_math_score + griffin_average_reading_score)/2\n",
    "griffin_math = griffin_student_df[(griffin_student_df['math_score']>70)].count()[\"name\"]\n",
    "griffin_percent_passing_math = passing_math/griffin_student_counts\n",
    "griffin_reading = griffin_student_df[(griffin_student_df['reading_score']>70)].count()[\"name\"]\n",
    "griffin_percent_passing_reading = passing_reading/griffin_student_counts\n",
    "griffin_type = schools_df.loc[schools_df[\"name\"] == \"Griffin High School\", \"type\"]\n",
    "griffin_budget = schools_df.loc[schools_df[\"name\"] == \"Griffin High School\", \"budget\"]"
   ]
