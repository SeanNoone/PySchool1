   "cell_type": "code",
   "execution_count": 129,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "pena_student_counts = pena_student_df[\"Student ID\"].count()\n",
    "pena_average_math_score = pena_student_df[\"math_score\"].mean()\n",
    "pena_average_reading_score = pena_student_df[\"reading_score\"].mean()\n",
    "pena_average = (pena_average_math_score + pena_average_reading_score)/2\n",
    "pena_math = pena_student_df[(pena_student_df['math_score']>70)].count()[\"name\"]\n",
    "pena_percent_passing_math = passing_math/pena_student_counts\n",
    "pena_reading = pena_student_df[(pena_student_df['reading_score']>70)].count()[\"name\"]\n",
    "pena_percent_passing_reading = passing_reading/pena_student_counts\n",
    "pena_type = schools_df.loc[schools_df[\"name\"] == \"Pena High School\", \"type\"]\n",
    "pena_budget = schools_df.loc[schools_df[\"name\"] == \"Pena High School\", \"budget\"]"
   ]
